<?xml version="1.0" encoding="utf-8"?>
<RelativeLayout xmlns:android="http://schemas.android.com/apk/res/android"
    xmlns:tools="http://schemas.android.com/tools"
    android:layout_width="match_parent"
    android:layout_height="wrap_content"
    android:layout_centerInParent="true"
    android:layout_centerHorizontal="true"
    >

    <RelativeLayout
        android:background="@color/white"
        android:layout_marginStart="@dimen/dp_10"
        android:layout_marginEnd="@dimen/dp_10"
        android:paddingLeft="@dimen/dp_20"
        android:paddingRight="@dimen/dp_20"
        android:layout_width="match_parent"
        android:layout_height="wrap_content">

        <RelativeLayout
            android:id="@+id/rl_pay_info"
            android:layout_width="match_parent"
            android:layout_height="wrap_content"
            android:layout_marginTop="@dimen/dp_10"
            android:layout_marginBottom="10dp"
            android:visibility="gone"
            >

            <TextView
                android:id="@+id/exam_type"
                android:layout_width="wrap_content"
                android:layout_height="wrap_content"
                android:text="考试类型:"
                android:textColor="@color/common_black"
                android:textSize="@dimen/sp_14"
                android:visibility="gone" />

            <TextView
                android:id="@+id/exam_type_detail"
                android:layout_width="wrap_content"
                android:layout_height="wrap_content"
                android:layout_marginStart="@dimen/dp_4"
                android:layout_toEndOf="@+id/exam_type"
                android:textColor="@color/text_common_gray"
                android:textSize="@dimen/sp_14"
                android:visibility="gone" />

            <TextView
                android:visibility="gone"
                android:id="@+id/enter_area"
                android:layout_width="wrap_content"
                android:layout_height="wrap_content"
                android:layout_below="@+id/exam_type"
                android:layout_marginTop="@dimen/dp_6"
                android:text="报考区域:"
                android:textColor="@color/common_black"
                android:textSize="@dimen/sp_14" />

            <TextView
                android:visibility="gone"
                android:id="@+id/order_number_area"
                android:layout_width="wrap_content"
                android:layout_height="wrap_content"
                android:layout_below="@+id/exam_type"
                android:layout_marginStart="@dimen/dp_4"
                android:layout_marginTop="@dimen/dp_6"
                android:layout_toEndOf="@+id/enter_area"
                android:textColor="@color/text_common_gray"
                android:textSize="@dimen/sp_14" />

            <TextView
                android:id="@+id/tv_xiangmu_name"
                android:layout_width="wrap_content"
                android:layout_height="wrap_content"
                android:layout_below="@id/enter_area"
                android:layout_marginTop="@dimen/dp_6"
                android:lines="1"
                android:maxLines="1"
                android:text="学校名称:"
                android:textColor="@color/common_black"
                android:textSize="@dimen/sp_14" />

            <TextView
                android:id="@+id/xiangmu_detail"
                android:layout_width="wrap_content"
                android:layout_height="wrap_content"
                android:layout_below="@id/enter_area"
                android:layout_marginStart="@dimen/dp_4"
                android:layout_marginLeft="@dimen/dp_20"
                android:layout_marginTop="@dimen/dp_6"
                android:layout_toRightOf="@id/tv_xiangmu_name"
                android:ellipsize="end"
                android:lines="1"
                android:maxLines="1"
                android:textColor="@color/text_common_gray"
                android:textSize="@dimen/dp_14" />

            <TextView
                android:id="@+id/order_pay_leixing"
                android:layout_width="wrap_content"
                android:layout_height="wrap_content"
                android:layout_below="@id/tv_xiangmu_name"
                android:layout_marginTop="@dimen/dp_6"
                android:lines="1"
                android:maxLines="1"
                android:text="学       历:"
                android:textColor="@color/common_black" />

            <TextView
                android:id="@+id/postion_detail"
                android:layout_width="wrap_content"
                android:layout_height="wrap_content"
                android:layout_below="@id/tv_xiangmu_name"
                android:layout_marginLeft="4dp"
                android:layout_marginTop="@dimen/dp_6"
                android:layout_toRightOf="@id/order_pay_leixing"
                android:ellipsize="end"
                android:lines="1"
                android:maxLines="1"
                android:textColor="@color/text_common_gray"
                android:textSize="@dimen/dp_14" />

            <TextView
                android:id="@+id/order_pay_type"
                android:layout_width="wrap_content"
                android:layout_height="wrap_content"
                android:layout_below="@id/order_pay_leixing"
                android:layout_marginTop="@dimen/dp_6"
                android:lines="1"
                android:maxLines="1"
                android:text="专业名称:"
                android:textColor="@color/common_black"
                android:textSize="@dimen/dp_14" />

            <TextView
                android:id="@+id/tv_user_name"
                android:layout_width="wrap_content"
                android:layout_height="wrap_content"
                android:layout_below="@id/order_pay_leixing"
                android:layout_marginLeft="@dimen/dp_4"
                android:layout_marginTop="@dimen/dp_6"
                android:layout_toRightOf="@id/order_pay_type"
                android:ellipsize="end"
                android:lines="1"
                android:maxLines="1"
                android:textColor="@color/text_common_gray"
                android:textSize="@dimen/dp_14" />

            <TextView
                android:id="@+id/stu_name"
                android:layout_width="wrap_content"
                android:layout_height="wrap_content"
                android:layout_below="@id/order_pay_type"
                android:layout_marginTop="@dimen/dp_6"
                android:lines="1"
                android:maxLines="1"
                android:text="姓       名:"
                android:textColor="@color/common_black"
                android:textSize="@dimen/dp_14" />

            <TextView
                android:id="@+id/stu_name_detail"
                android:layout_width="wrap_content"
                android:layout_height="wrap_content"
                android:layout_below="@id/order_pay_type"
                android:layout_marginLeft="@dimen/dp_4"
                android:layout_marginTop="@dimen/dp_6"
                android:layout_toRightOf="@id/stu_name"
                android:ellipsize="end"
                android:lines="1"
                android:maxLines="1"
                android:textColor="@color/text_common_gray"
                android:textSize="@dimen/dp_14" />

            <TextView
                android:id="@+id/xue_card"
                android:layout_width="wrap_content"
                android:layout_height="wrap_content"
                android:layout_below="@id/stu_name"
                android:layout_marginTop="@dimen/dp_6"
                android:lines="1"
                android:maxLines="1"
                android:text="身份证号:"
                android:textColor="@color/common_black"
                android:textSize="@dimen/dp_14" />

            <TextView
                android:id="@+id/xue_card_detail"
                android:layout_width="wrap_content"
                android:layout_height="wrap_content"
                android:layout_below="@id/stu_name"
                android:layout_marginLeft="@dimen/dp_4"
                android:layout_marginTop="@dimen/dp_6"
                android:layout_toRightOf="@id/census"
                android:ellipsize="end"
                android:lines="1"
                android:maxLines="1"
                android:textColor="@color/text_common_gray"
                android:textSize="@dimen/dp_14" />

            <TextView
                android:id="@+id/census"
                android:layout_width="wrap_content"
                android:layout_height="wrap_content"
                android:layout_below="@id/xue_card"
                android:layout_marginTop="@dimen/dp_6"
                android:lines="1"
                android:maxLines="1"
                android:text="户       籍:"
                android:textColor="@color/common_black"
                android:textSize="@dimen/dp_14" />

            <TextView
                android:id="@+id/census_detail"
                android:layout_width="wrap_content"
                android:layout_height="wrap_content"
                android:layout_below="@id/xue_card"
                android:layout_marginLeft="@dimen/dp_4"
                android:layout_marginTop="@dimen/dp_6"
                android:layout_toRightOf="@id/census"
                android:ellipsize="end"
                android:lines="1"
                android:maxLines="1"
                android:textColor="@color/text_common_gray"
                android:textSize="@dimen/dp_14" />

            <TextView
                android:id="@+id/order_pay_money"
                android:layout_width="wrap_content"
                android:layout_height="wrap_content"
                android:layout_below="@id/census"
                android:layout_marginTop="@dimen/dp_6"
                android:lines="1"
                android:maxLines="1"
                android:text="下单金额:"
                android:textColor="@color/common_black"
                android:textSize="@dimen/dp_14" />

            <TextView
                android:id="@+id/tv_position"
                android:layout_width="wrap_content"
                android:layout_height="wrap_content"
                android:layout_below="@id/census"
                android:layout_marginLeft="@dimen/dp_4"
                android:layout_marginTop="@dimen/dp_6"
                android:layout_toRightOf="@id/order_pay_money"
                android:ellipsize="end"
                android:lines="1"
                android:maxLines="1"
                android:textColor="@color/text_common_balck"
                android:textSize="@dimen/dp_14" />

        </RelativeLayout>


        <RelativeLayout
            android:id="@+id/rl_xiezhu"
            android:layout_width="match_parent"
            android:layout_height="wrap_content"
            android:layout_marginTop="@dimen/dp_10"
            android:layout_marginBottom="10dp"
            android:visibility="gone">

            <TextView
                android:id="@+id/xie_pro"
                android:layout_width="wrap_content"
                android:layout_height="wrap_content"
                android:text="项目名称:"
                android:textColor="@color/common_black"
                android:textSize="@dimen/sp_14" />

            <TextView
                android:id="@+id/xie_pro_detail"
                android:layout_width="wrap_content"
                android:layout_height="wrap_content"
                android:layout_marginStart="@dimen/dp_4"
                android:layout_toEndOf="@+id/xie_pro"
                android:textColor="@color/text_common_gray"
                android:textSize="@dimen/sp_14" />

            <TextView
                android:id="@+id/xie_area"
                android:layout_width="wrap_content"
                android:layout_height="wrap_content"
                android:layout_below="@+id/xie_pro"
                android:layout_marginTop="@dimen/dp_6"
                android:text="报考区域:"
                android:textColor="@color/common_black"
                android:textSize="@dimen/sp_14" />

            <TextView
                android:id="@+id/xie_area_detail"
                android:layout_width="wrap_content"
                android:layout_height="wrap_content"
                android:layout_below="@+id/xie_pro"
                android:layout_marginStart="@dimen/dp_4"
                android:layout_marginTop="@dimen/dp_6"
                android:layout_toEndOf="@+id/xie_area"
                android:textColor="@color/text_common_gray"
                android:textSize="@dimen/sp_14" />

            <TextView
                android:id="@+id/xie_name"
                android:layout_width="wrap_content"
                android:layout_height="wrap_content"
                android:layout_below="@id/xie_area"
                android:layout_marginTop="@dimen/dp_6"
                android:lines="1"
                android:maxLines="1"
                android:text="姓       名:"
                android:textColor="@color/common_black"
                android:textSize="@dimen/sp_14" />

            <TextView
                android:id="@+id/xie_name_detail"
                android:layout_width="wrap_content"
                android:layout_height="wrap_content"
                android:layout_below="@id/xie_area"
                android:layout_marginStart="@dimen/dp_4"
                android:layout_marginLeft="@dimen/dp_20"
                android:layout_marginTop="@dimen/dp_6"
                android:layout_toRightOf="@id/xie_name"
                android:ellipsize="end"
                android:lines="1"
                android:maxLines="1"
                android:textColor="@color/text_common_gray"
                android:textSize="@dimen/dp_14" />

            <TextView
                android:id="@+id/xie_card"
                android:layout_width="wrap_content"
                android:layout_height="wrap_content"
                android:layout_below="@id/xie_name"
                android:layout_marginTop="@dimen/dp_6"
                android:lines="1"
                android:maxLines="1"
                android:text="身份证号:"
                android:textColor="@color/common_black" />

            <TextView
                android:id="@+id/xie_card_detail"
                android:layout_width="wrap_content"
                android:layout_height="wrap_content"
                android:layout_below="@id/xie_name"
                android:layout_marginLeft="4dp"
                android:layout_marginTop="@dimen/dp_6"
                android:layout_toRightOf="@id/xie_card"
                android:ellipsize="end"
                android:lines="1"
                android:maxLines="1"
                android:textColor="@color/text_common_gray"
                android:textSize="@dimen/dp_14" />

            <TextView
                android:id="@+id/xie_census"
                android:layout_width="wrap_content"
                android:layout_height="wrap_content"
                android:layout_below="@id/xie_card"
                android:layout_marginTop="@dimen/dp_6"
                android:lines="1"
                android:maxLines="1"
                android:text="户       籍:"
                android:textColor="@color/common_black"
                android:textSize="@dimen/dp_14" />

            <TextView
                android:id="@+id/xie_census_detail"
                android:layout_width="wrap_content"
                android:layout_height="wrap_content"
                android:layout_below="@id/xie_card"
                android:layout_marginLeft="@dimen/dp_4"
                android:layout_marginTop="@dimen/dp_6"
                android:layout_toRightOf="@id/xie_census"
                android:ellipsize="end"
                android:lines="1"
                android:maxLines="1"
                android:textColor="@color/text_common_gray"
                android:textSize="@dimen/dp_14" />

            <TextView
                android:id="@+id/xie_price"
                android:layout_width="wrap_content"
                android:layout_height="wrap_content"
                android:layout_below="@id/xie_census"
                android:layout_marginTop="@dimen/dp_6"
                android:lines="1"
                android:maxLines="1"
                android:text="下单金额:"
                android:textColor="@color/common_black"
                android:textSize="@dimen/dp_14" />

            <TextView
                android:id="@+id/xie_price_detail"
                android:layout_width="wrap_content"
                android:layout_height="wrap_content"
                android:layout_below="@id/xie_census"
                android:layout_marginLeft="@dimen/dp_4"
                android:layout_marginTop="@dimen/dp_6"
                android:layout_toRightOf="@id/xie_price"
                android:ellipsize="end"
                android:lines="1"
                android:maxLines="1"
                android:textColor="@color/text_common_gray"
                android:textSize="@dimen/dp_14" />

        </RelativeLayout>


        <RelativeLayout
            android:id="@+id/rl_other"
            android:layout_width="match_parent"
            android:layout_height="wrap_content"
            android:layout_marginTop="@dimen/dp_10"
            android:layout_marginBottom="10dp">

            <TextView
                android:id="@+id/other_pro"
                android:layout_width="wrap_content"
                android:layout_height="wrap_content"
                android:text="证书名称:"
                android:textColor="@color/common_black"
                android:textSize="@dimen/sp_14" />

            <TextView
                android:id="@+id/other_pro_detail"
                android:layout_width="wrap_content"
                android:layout_height="wrap_content"
                android:layout_marginStart="@dimen/dp_4"
                android:layout_toEndOf="@+id/other_pro"
                android:textColor="@color/text_common_gray"
                android:textSize="@dimen/sp_14" />

            <TextView
                android:id="@+id/other_name"
                android:layout_width="wrap_content"
                android:layout_height="wrap_content"
                android:layout_below="@id/other_pro"
                android:layout_marginTop="@dimen/dp_6"
                android:lines="1"
                android:maxLines="1"
                android:text="姓       名:"
                android:textColor="@color/common_black"
                android:textSize="@dimen/sp_14" />

            <TextView
                android:id="@+id/other_name_detail"
                android:layout_width="wrap_content"
                android:layout_height="wrap_content"
                android:layout_below="@id/other_pro"
                android:layout_marginStart="@dimen/dp_4"
                android:layout_marginLeft="@dimen/dp_20"
                android:layout_marginTop="@dimen/dp_6"
                android:layout_toRightOf="@id/other_name"
                android:ellipsize="end"
                android:lines="1"
                android:maxLines="1"
                android:textColor="@color/text_common_gray"
                android:textSize="@dimen/dp_14" />


            <TextView
                android:id="@+id/other_census"
                android:layout_width="wrap_content"
                android:layout_height="wrap_content"
                android:layout_below="@id/other_name"
                android:layout_marginTop="@dimen/dp_6"
                android:lines="1"
                android:maxLines="1"
                android:text="户       籍:"
                android:textColor="@color/common_black"
                android:textSize="@dimen/dp_14" />

            <TextView
                android:id="@+id/other_census_detail"
                android:layout_width="wrap_content"
                android:layout_height="wrap_content"
                android:layout_below="@id/other_name"
                android:layout_marginLeft="@dimen/dp_4"
                android:layout_marginTop="@dimen/dp_6"
                android:layout_toRightOf="@id/other_census"
                android:ellipsize="end"
                android:lines="1"
                android:maxLines="1"
                android:textColor="@color/text_common_gray"
                android:textSize="@dimen/dp_14" />

            <TextView
                android:id="@+id/other_price"
                android:layout_width="wrap_content"
                android:layout_height="wrap_content"
                android:layout_below="@id/other_census"
                android:layout_marginTop="@dimen/dp_6"
                android:lines="1"
                android:maxLines="1"
                android:text="下单金额:"
                android:textColor="@color/common_black"
                android:textSize="@dimen/dp_14" />

            <TextView
                android:id="@+id/other_price_detail"
                android:layout_width="wrap_content"
                android:layout_height="wrap_content"
                android:layout_below="@id/other_census"
                android:layout_marginLeft="@dimen/dp_4"
                android:layout_marginTop="@dimen/dp_6"
                android:layout_toRightOf="@id/other_price"
                android:ellipsize="end"
                android:lines="1"
                android:maxLines="1"
                android:textColor="@color/text_common_gray"
                android:textSize="@dimen/dp_14" />

        </RelativeLayout>


        <RelativeLayout
            android:id="@+id/rl_wang"
            android:layout_width="match_parent"
            android:layout_height="wrap_content"
            android:layout_marginTop="@dimen/dp_10"
            android:layout_marginBottom="5dp"
            android:visibility="gone">

            <TextView
                android:id="@+id/wang_pro"
                android:layout_width="wrap_content"
                android:layout_height="wrap_content"
                android:text="项目名称:"
                android:textColor="@color/common_black"
                android:textSize="@dimen/sp_14" />

            <TextView
                android:id="@+id/wang_pro_detail"
                android:layout_width="wrap_content"
                android:layout_height="wrap_content"
                android:layout_marginStart="@dimen/dp_4"
                android:layout_toEndOf="@+id/wang_pro"
                android:textColor="@color/text_common_gray"
                android:textSize="@dimen/sp_14" />

            <TextView
                android:id="@+id/wang_name"
                android:layout_width="wrap_content"
                android:layout_height="wrap_content"
                android:layout_below="@id/wang_pro"
                android:layout_marginTop="@dimen/dp_6"
                android:lines="1"
                android:maxLines="1"
                android:text="班       型:"
                android:textColor="@color/common_black"
                android:textSize="@dimen/sp_14" />

            <TextView
                android:id="@+id/wang_name_detail"
                android:layout_width="wrap_content"
                android:layout_height="wrap_content"
                android:layout_below="@id/wang_pro"
                android:layout_marginStart="@dimen/dp_4"
                android:layout_marginLeft="@dimen/dp_20"
                android:layout_marginTop="@dimen/dp_6"
                android:layout_toRightOf="@id/wang_name"
                android:ellipsize="end"
                android:lines="1"
                android:maxLines="1"
                android:textColor="@color/text_common_gray"
                android:textSize="@dimen/dp_14" />


            <TextView
                android:id="@+id/wang_price"
                android:layout_width="wrap_content"
                android:layout_height="wrap_content"
                android:layout_below="@id/wang_name"
                android:layout_marginTop="@dimen/dp_6"
                android:lines="1"
                android:maxLines="1"
                android:text="下单金额:"
                android:textColor="@color/common_black"
                android:textSize="@dimen/dp_14" />

            <TextView
                android:id="@+id/wang_price_detail"
                android:layout_width="wrap_content"
                android:layout_height="wrap_content"
                android:layout_below="@id/wang_name"
                android:layout_marginLeft="@dimen/dp_4"
                android:layout_marginTop="@dimen/dp_6"
                android:layout_toRightOf="@id/wang_price"
                android:ellipsize="end"
                android:lines="1"
                android:maxLines="1"
                android:textColor="@color/text_common_gray"
